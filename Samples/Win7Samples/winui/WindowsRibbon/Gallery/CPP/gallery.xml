    <Application.Views>
        <Ribbon>
            <Ribbon.Tabs>
                <Tab CommandName="cmdTabHome">
                    <Group CommandName="cmdShapesGroup" SizeDefinition="InRibbonGalleryAndBigButton">
                        <InRibbonGallery CommandName="cmdShapes" Type="Items" ItemHeight="32" ItemWidth="32" TextPosition="Bottom" MaxColumns="3" MaxRows="1" MinColumnsMedium="1"/>
                        <DropDownGallery CommandName="cmdSizeAndColor" TextPosition="Hide" Type="Commands" ItemHeight="32" ItemWidth="32">
                            <DropDownGallery.MenuLayout>
                                <FlowMenuLayout Rows="2" Columns="3" Gripper="None"/>
                            </DropDownGallery.MenuLayout>
                        </DropDownGallery>
                    </Group>
                    <Group CommandName="cmdBorderGroup">
                        <SizeDefinition>
                            <ControlNameMap>
                                <ControlNameDefinition Name="gallery"/>
                                <ControlNameDefinition Name="combobox"/>
                            </ControlNameMap>
                            <GroupSizeDefinition Size="Large">
                                <ControlSizeDefinition ControlName="gallery" ImageSize="Large" IsImageVisible="true" IsLabelVisible="true"/>
                                <ControlSizeDefinition ControlName="combobox" ImageSize="Small" IsImageVisible="true" IsLabelVisible="true"/>
                            </GroupSizeDefinition>
                            <GroupSizeDefinition Size="Medium">
                                <ControlSizeDefinition ControlName="gallery" ImageSize="Large" IsImageVisible="true" IsLabelVisible="true"/>
                                <ControlSizeDefinition ControlName="combobox" ImageSize="Small" IsImageVisible="true" IsLabelVisible="true"/>
                            </GroupSizeDefinition>
                            <GroupSizeDefinition Size="Small">
                                <ControlSizeDefinition ControlName="gallery" ImageSize="Large" IsImageVisible="true" IsLabelVisible="true"/>
                                <ControlSizeDefinition ControlName="combobox" ImageSize="Small" IsImageVisible="true" IsLabelVisible="true"/>
                            </GroupSizeDefinition>
                        </SizeDefinition>
                        <SplitButtonGallery CommandName="cmdBorderStyles" TextPosition="Hide" Type="Items" ItemHeight="32" ItemWidth="32">
                            <SplitButtonGallery.MenuLayout>
                                <VerticalMenuLayout/>
                            </SplitButtonGallery.MenuLayout>
                        </SplitButtonGallery>
                        <ComboBox CommandName="cmdBorderSizes" IsEditable="true"/>
                    </Group>
                    <Group CommandName="cmdLayoutGroup">
                        <ComboBox CommandName="cmdLayouts" IsEditable="false"/>
                    </Group>
                </Tab>
            </Ribbon.Tabs>
        </Ribbon>
    </Application.Views>
</Application>